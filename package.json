{
  "name": "next-js-blog-template",
  "version": "0.0.1",
  "description": "Blog template with Next.js.",
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yopinoji/next-js-blog-template.git"
  },
  "homepage": "https://next-playground.vercel.app",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/react": "11.7.1",
    "esbuild": "0.14.2",
    "github-slugger": "1.4.0",
    "gray-matter": "4.0.3",
    "mdx-bundler": "7.0.0",
    "next": "12.1.0",
    "next-themes": "0.0.15",
    "postcss": "8.4.8",
    "preact": "10.5.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-autolink-headings": "6.1.0",
    "rehype-prism-plus": "1.3.2",
    "rehype-slug": "5.0.1",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/rss": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "@next/bundle-analyzer": "12.0.7",
    "@svgr/webpack": "6.2.1",
    "dedent": "0.7.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "husky": "7.0.2",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
