{
  "name": "next-js-blog-template",
  "version": "0.0.2",
  "description": "Blog template with Next.js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yopinoji/next-js-blog-template.git"
  },
  "homepage": "https://next-playground.vercel.app",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@emotion/react": "11.10.0",
    "esbuild": "0.14.54",
    "github-slugger": "1.4.0",
    "gray-matter": "4.0.3",
    "mdx-bundler": "9.0.1",
    "next": "12.2.5",
    "next-seo": "5.5.0",
    "postcss": "8.4.16",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-prism-plus": "1.5.0",
    "rehype-slug": "5.0.0",
    "rehype-toc": "3.0.2",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.5",
    "@svgr/webpack": "6.3.1",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "18.7.13",
    "@types/react": "17.0.48",
    "@types/react-dom": "17.0.17",
    "@types/rss": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "file-loader": "6.2.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.7.1",
    "typescript": "4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
